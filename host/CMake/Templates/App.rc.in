#include <windows.h>

IDI_ICON1	ICON	DISCARDABLE	"@PROJECT_WIN32_ICO@"

VS_VERSION_INFO VERSIONINFO
	FILEVERSION @VERSION_MAJOR@,@VERSION_MINOR@,@VERSION_PATCH@,@BUILD_ID@
	PRODUCTVERSION @VERSION_MAJOR@,@VERSION_MINOR@,@VERSION_PATCH@,@BUILD_ID@
	FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
	FILEFLAGS VS_FF_DEBUG
#else
	FILEFLAGS 0x0L
#endif
	FILEOS VOS__WINDOWS32
	FILETYPE VFT_DLL
	FILESUBTYPE 0x0L
	BEGIN
		BLOCK "StringFileInfo"
		BEGIN
			BLOCK "040904b0"
			BEGIN
				VALUE "CompanyName", "@PROJECT_COMPANY@\0"
				VALUE "FileDescription", "@PROJECT_DESCRIPTION@\0"
				VALUE "FileVersion", "@VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@.@BUILD_ID@\0"
				VALUE "LegalCopyright", "@PROJECT_COPYRIGHT@\0"
				VALUE "OriginalFilename", "@PROJECT_DESCRIPTION@.exe\0"
				VALUE "ProductName", "@PROJECT_DESCRIPTION@\0"
				VALUE "ProductVersion", "@VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@\0"
			END
		END
		BLOCK "VarFileInfo"
		BEGIN
			VALUE "Translation", 0x0409, 1200
		END
	END
/* End of Version info */
