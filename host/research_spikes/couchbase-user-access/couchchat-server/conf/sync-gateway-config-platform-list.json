{
  "logging": {
    "console": {
      "enabled": true,
      "log_level": "info",
      "log_keys": ["*"]
    }
  },
  "databases": {
    "platform-list": {
      "server": "http://couchbase:8091",
      "bucket": "platform-list",
      "username": "sync_gateway",
      "password": "sync_gateway",
      "enable_shared_bucket_access": true,
      "import_docs": "continuous",
      "num_index_replicas": 0,

      "users": {
        "GUEST": {
          "disabled": false, "admin_channels": ["*"]
        },
        "username": {
          "disabled": false, "admin_channels": ["*"], "password":"password"
        }
      },

      "sync":
      `
        function (doc, oldDoc) {
          if (doc._id == "user_access_map") {
            channel("user_access_map")
            return
          }

          if (doc.channels_available) {
            channel(doc.channels_available)
            return
          }
        }
      `
    }
  }
}
