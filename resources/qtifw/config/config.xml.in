<Installer>
    <Name>Strata</Name>
    <Version>1.0.0-1</Version>
    <Title>onsemi: Strata</Title>
    <Publisher>onsemi</Publisher>
    <ProductUrl>https://www.onsemi.com/strata</ProductUrl>
    <InstallerApplicationIcon>onsemi-icon</InstallerApplicationIcon>
    <InstallerWindowIcon>onsemi-icon.png</InstallerWindowIcon>
    <Logo>strata-logo.png</Logo>
    <WizardStyle>Modern</WizardStyle>
    <WizardDefaultWidth>840</WizardDefaultWidth>
    <WizardDefaultHeight>480</WizardDefaultHeight>
    <WizardMinimumWidth>840</WizardMinimumWidth>
    <WizardMinimumHeight>480</WizardMinimumHeight>
    <StartMenuDir>onsemi</StartMenuDir>
    <TargetDir>@ApplicationsDirX64@/onsemi/Strata</TargetDir>
    <MaintenanceToolName>Strata Maintenance Tool</MaintenanceToolName>
    <AllowNonAsciiCharacters>true</AllowNonAsciiCharacters>
    <InstallActionColumnVisible>true</InstallActionColumnVisible>
    <DisableAuthorizationFallback>true</DisableAuthorizationFallback>
    <ControlScript>installscript.qs</ControlScript>
    <StyleSheet>style.qss</StyleSheet>
    @STRATA_OTA_REPOSITORIES@
    <ProductImages>
        <ProductImage><Image>onsemi-banner-1.png</Image></ProductImage>
        <ProductImage><Image>onsemi-banner-2.png</Image></ProductImage>
        <ProductImage><Image>onsemi-banner-3.png</Image></ProductImage>
        <ProductImage><Image>onsemi-banner-4.png</Image></ProductImage>
    </ProductImages>
</Installer>
