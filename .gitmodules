[submodule "cloud/couchbase-lite-core"]
	path = cloud/couchbase-lite-core
	url = https://github.com/couchbase/couchbase-lite-core.git
[submodule "shared/cpp-base64"]
	path = shared/cpp-base64
	url = https://github.com/ReneNyffenegger/cpp-base64.git
[submodule "host/ext_libs/threadpool"]
	path = host/ext_libs/threadpool
	url = https://github.com/progschj/ThreadPool
[submodule "extern/spdlog"]
	path = extern/spdlog
	url = https://github.com/gabime/spdlog.git
[submodule "extern/rapidjson"]
	path = extern/rapidjson
	url = https://github.com/Tencent/rapidjson.git
[submodule "extern/googletest"]
	path = extern/googletest
	url = https://github.com/google/googletest.git
[submodule "extern/couchbase-lite-cpp"]
	path = extern/couchbase-lite-cpp
	url = https://github.com/stratadeveloperstudio/couchbase-lite-cpp.git
	ignore = dirty
[submodule "extern/qtmqtt"]
	path = extern/qtmqtt
	url = https://github.com/qt/qtmqtt.git
	ignore = all
[submodule "extern/couchbase-lite-C"]
	path = extern/couchbase-lite-C
	url = https://github.com/couchbaselabs/couchbase-lite-C.git
	ignore = dirty
