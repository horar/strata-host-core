<Package>
    <DisplayName>@PROJECT_DESCRIPTION@</DisplayName>
    <Description>The Visual C++ Redistributable Packages install run-time components that are required to run C++ applications built using Visual Studio 2017</Description>
    <Version>@VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@.@VERSION_OPTIONAL@-@BUILD_ID@</Version>
    <ForcedInstallation>true</ForcedInstallation>
    <SortingPriority>50</SortingPriority>
    <!--we need admin rights to install this component-->
    <RequiresAdminRights>true</RequiresAdminRights>
    <Script>installscript.js</Script>
    <ReleaseDate>@BUILD_TIMESTAMP@</ReleaseDate>
    <Virtual>True</Virtual>
    <Licenses>
        <License name="Microsoft VS 2017 Tools, Add-ONs and Extensions License Agreement" file="vcredist_license.html" />
    </Licenses>
</Package>
